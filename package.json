{
  "name": "bootstrap-components",
  "description": "Common vue components for building different web applications",
  "version": "2.1.1-dev.0",
  "type": "module",
  "license": "Apache-2.0",
  "main": "./src/main.js",
  "style": "dist/bootstrap-components.css",
  "files": [
    "dist/bootstrap-components.css",
    "dist/bootstrap-components.es.js",
    "dist/bootstrap-components.umd.js"
  ],
  "exports": {
    ".": {
      "import": "./dist/bootstrap-components.es.js",
      "require": "./dist/bootstrap-components.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:library": "cross-env BUILD_MODE=library vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "lint": "eslint . --fix",
    "bump:dev": "../bump-dev-version.sh bootstrap-components",
    "remove:dev": "../npm-version-release.sh bootstrap-components"
  },
  "dependencies": {
    "bootstrap": "^5.3.6",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.10",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/eslint-plugin": "^1.1.44",
    "@vue/test-utils": "^2.4.6",
    "cross-env": "^7.0.3",
    "cypress": "^14.3.3",
    "eslint": "^9.26.0",
    "eslint-plugin-cypress": "^4.3.0",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^26.1.0",
    "start-server-and-test": "^2.0.11",
    "vite": "^6.4.1",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vitest": "^3.1.3"
  },
  "publishConfig": {
    "registry": "https://artifacts.cibseven.org/repository/npm-hosted/"
  }
}
