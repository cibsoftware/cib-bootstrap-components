{
  "name": "@cib/bootstrap-components",
  "description": "Common vue components for building different web applications",
  "version": "1.0.2-dev.0",
  "type": "module",
  "license": "Apache-2.0",
  "main": "dist/bootstrap-components.umd.js",
  "module": "dist/bootstrap-components.es.js",
  "style": "dist/bootstrap-components.css",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/bootstrap-components.es.js",
      "require": "./dist/bootstrap-components.umd.js"
    },
    "./dist/style.css": "./dist/bootstrap-components.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:jenkinsfile": "vitest run --coverage --reporter=default --reporter=junit --outputFile=target/vitest-reports/TEST-front-end.xml",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --reporter=default --reporter=junit --outputFile=target/vitest-reports/TEST-front-end.xml",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "publish:dev": "npm run build && npm version prerelease --preid=dev --force && npm publish --registry https://nexus.cib.de/repository/npm-hosted-dev/ --tag dev"
  },
  "dependencies": {
    "bootstrap": "5.3.8",
    "vue": "3.5.16",
    "vue-i18n": "11.1.10",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "9.26.0",
    "@vitejs/plugin-vue": "5.2.4",
    "@vitest/coverage-istanbul": "3.1.3",
    "@vitest/eslint-plugin": "1.1.44",
    "@vue/test-utils": "2.4.6",
    "cross-env": "7.0.3",
    "cypress": "14.3.3",
    "eslint": "9.39.1",
    "eslint-plugin-cypress": "4.3.0",
    "eslint-plugin-vue": "9.32.0",
    "jsdom": "26.1.0",
    "start-server-and-test": "2.0.11",
    "vite": "6.4.1",
    "vite-plugin-vue-devtools": "7.7.6",
    "vitest": "3.1.3"
  }
}
